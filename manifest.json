{
  "manifest_version": 2,

  "name": "To Do List Plugin",
  "description": "This extension will help to manage your daily tasks",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
        "tabs",
        "alarms"
    ]
}